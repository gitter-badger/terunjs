{
    "config":{},
    "entity":{
        "name":"Pessoa",
        "attributes":{
            "nome": {
                "type": "text"
            },
            "idade":{
                "type": "number",
                "placeholder": "my age is",
                "label": "Write your age:",
                "minlength": 10,
                "min": 10
            },
            "casado":{
                "type": "boolean",
                "label": "Is married:"
            },
            "veiculos":{
                "type": "hasMany",
                "label":"Selecione seu veiculo",
                "reference": {
                    "entity": "veiculo"
                }
            },
            "uber":{
                "type": "hasOne",
                "label":"Qual teu uber?",
                "reference": {
                    "entity": "veiculo"
                }
            }
        }
    }
}